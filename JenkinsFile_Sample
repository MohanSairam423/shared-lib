@Library('shared-lib')_
pipFunc() {

	stage('Print Build Info') {
	    //snDevOpsStep
		printBuildinfo {
			name = "Sample Name"
		}
	}
	stage('Disable balancer') {
	   // snDevOpsStep
	   	snDevOpsChange()
		disableBalancerUtils()
	}
	stage('Deploy') {
	    //snDevOpsStep
		deploy()
	}
	stage('Enable balancer') {
	    //snDevOpsStep
	    	snDevOpsChange()
		enableBalancerUtils()
	}
	stage('Check Status') {
	    //snDevOpsStep 'e4407d439f723300a1af5bb0657fcf6b'
	    snDevOpsChange()
		checkStatus()
	}
}
